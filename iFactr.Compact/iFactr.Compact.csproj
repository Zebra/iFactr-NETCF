  <PropertyGroup>
    <PreBuildEvent>git rev-list --count HEAD &gt; buildrev.txt &amp;&amp; set /p BUILDREV=&lt;buildrev.txt &amp;&amp; del buildrev.txt
git rev-parse --abbrev-ref HEAD &gt; branch.txt &amp;&amp; set /p BRANCH=&lt;branch.txt &amp;&amp; del branch.txt
git rev-parse --short HEAD &gt; commithash.txt &amp;&amp; set /p COMMITHASH=&lt;commithash.txt &amp;&amp; del commithash.txt

setlocal EnableDelayedExpansion
set LINES=0
for /f "delims==" %25%25I in ($(ProjectDir)Properties\AssemblyInfo.cs) do (
    set /a LINES=LINES+1    
)
set /a LINES=LINES-3

call:PrintFirstNLine &gt; output.txt

FOR /F "tokens=*" %%A IN ('DATE/T') DO FOR %%B IN (%%A) DO SET Today=%%B
FOR /F "tokens=1-3 delims=/-" %%A IN ("%Today%") DO (
	SET DayMonth=%%A
	SET MonthDay=%%B
	SET Year=%%C
)

echo "%Year%")]&gt;&gt; output.txt
echo [assembly: AssemblyVersion("6.5.3.%BUILDREV%")]&gt;&gt; output.txt
echo [assembly: AssemblyInformationalVersion("6.5.3.%BUILDREV%-%BRANCH%.%COMMITHASH%")]&gt;&gt; output.txt
move output.txt $(ProjectDir)Properties\AssemblyInfo.cs

goto EOF

:PrintFirstNLine
set cur=0
for /f "delims==" %25%25I in ($(ProjectDir)Properties\AssemblyInfo.cs) do (
    echo %25%25I
    set /a cur=cur+1    
    if "!cur!"=="%25LINES%25" goto EOF
) 

:EOF 

exit /b</PreBuildEvent>
    <PostBuildEvent>copy "$(TargetDir)$(TargetName).*" "$(SolutionDir)Distribution"</PostBuildEvent>
